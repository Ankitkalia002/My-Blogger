<h1>Listing customer_cards</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_cards.each do |customer_card| %>
      <tr>
        <td><%= link_to 'Show', customer_card %></td>
        <td><%= link_to 'Edit', edit_customer_card_path(customer_card) %></td>
        <td><%= link_to 'Destroy', customer_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

